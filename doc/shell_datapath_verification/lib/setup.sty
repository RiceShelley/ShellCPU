% Create New Title Page
\renewcommand{\maketitle}{
\begin{titlepage}
\center
	{\Huge \textbf{\@title}} \\
	\medskip 
	{\Large \@subtitle} \\
	\bigskip
	
	\begin{figure}[H]
		\centering
		\includegraphics[width=1.5in]{shell.png}
	\end{figure}
	\begin{center}
	     \@author  \\
	     \@date \\
	\end{center}
	\vspace{40pt}
	\Large \@subject \\
	\vfill
\end{titlepage}
}

